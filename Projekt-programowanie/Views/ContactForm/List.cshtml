@model IEnumerable<ProjektProgramowanie.Controllers.ContactFormController.ContactFormViewModel>

@{
    ViewData["Title"] = "Lista Formularzy Kontaktowych";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Tytuł</th>
            <th>Treść</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.Content</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>
