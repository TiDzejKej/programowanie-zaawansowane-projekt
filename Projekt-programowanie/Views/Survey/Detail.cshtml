@model ProjektProgramowanie.Models.Survey

@{
    ViewData["Title"] = "Survey Details";
}

<h1>@ViewData["Title"]</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Student ID</dt>
        <dd class="col-sm-10">@Model.StudentId</dd>

        <dt class="col-sm-2">Lesson ID</dt>
        <dd class="col-sm-10">@Model.LessonId</dd>

        <dt class="col-sm-2">Course Rating</dt>
        <dd class="col-sm-10">@Model.CourseRating</dd>

        <dt class="col-sm-2">Recommendation</dt>
        <dd class="col-sm-10">@Model.Recommendation</dd>
    </dl>
    <a href="@Url.Action("Index")" class="btn btn-secondary">Back to List</a>
</div>
