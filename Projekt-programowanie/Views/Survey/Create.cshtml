@model ProjektProgramowanie.Models.SurveyViewModel

@{
    ViewData["Title"] = "Add Survey";
}

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="LessonId" class="control-label"></label>
        <input asp-for="LessonId" class="form-control" readonly/>
        <span asp-validation-for="LessonId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="CourseRating" class="control-label"></label>
        <input asp-for="CourseRating" class="form-control" />
        <span asp-validation-for="CourseRating" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Recommendation" class="control-label"></label>
        <input asp-for="Recommendation" class="form-control" />
        <span asp-validation-for="Recommendation" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="StudentId" class="control-label"></label>
        <input asp-for="StudentId" class="form-control" readonly />
        <span asp-validation-for="StudentId" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
